<!DOCTYPE html>
<html>

<head>
    <title>Data Vault</title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script src="/javascripts/script.js"></script>
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <script src="https://code.jquery.com/jquery-3.5.1.min.js"></script>

</head>
<header>
    <%- include('../partials/admin-Homeheader'); %>
</header>

<body>
    <section>



        <% for(var i=0; i < data.length; i++){ %>
            <% if (data[i].Record.privileges.length!=0) { %>
                <% for(var j=0; j < data[i].Record.privileges.length; j++){ %>
                    <% if (data[i].Record.privileges[j].status==0) { %>
                        <div class="container mt-5">
                            <div class="row">
                                <div class="col-sm-6">
                                    <div class="card border-dark">
                                        <div class="card-body">
                                            <h5 class="card-header">
                                                <%= data[i].Record.name %>
                                            </h5>
                                            <p class="card-text">
                                                <%= data[i].Record.address %>
                                            </p>
                                            <p class="card-text">
                                                <%= data[i].Record.district%>
                                            </p>
                                            <p class="card-text">
                                                <%= data[i].Record.state %>
                                            </p>
                                            <p class="card-text">
                                                <%= data[i].Record.pincode %>
                                            </p>
                                            <p class="card-text">
                                                <%= data[i].Record.email %>
                                            </p>
                                            <p class="card-text">
                                                <%= data[i].Record.phone %>
                                            </p>
                                            <p class="card-text">
                                                <b><%= data[i].Record.owner %></b>
                                            </p>

                                            <a href="#" class="btn btn-success">Approve</a>
                                            <a href="#" class="btn btn-danger">Reject</a>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>


                        <% } %>
                            <% } %>
                                <% } %>
                                    <% }%>


    </section>

</body>

</html>

<script>
    function valForm() {
        console.log("Validating Form");
        return true;
    }
</script>